# LLM Providers Environment Variables
# Copy this file to .env and fill in your actual values

# =============================================================================
# AWS Bedrock Configuration
# =============================================================================
# AWS Region for Bedrock (default: us-east-1)
AWS_REGION=us-east-1

# AWS Credentials (required for Bedrock)
AWS_ACCESS_KEY_ID=your_aws_access_key_id
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key

# Bedrock Model Configuration
BEDROCK_MODEL=us.anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_PRIMARY_MODEL=us.anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_REGION=us-east-1

# Bedrock Fallback Models (comma-separated)
BEDROCK_FALLBACK_MODELS=us.anthropic.claude-3-haiku-20240307-v1:0,us.anthropic.claude-3-opus-20240229-v1:0

# Bedrock Cross-Provider Fallback (fallback to OpenAI if Bedrock fails)
BEDROCK_CROSS_FALLBACK_PROVIDER=openai
BEDROCK_CROSS_FALLBACK_MODELS=gpt-4o-mini,gpt-3.5-turbo
BEDROCK_OPENAI_FALLBACK_MODELS=gpt-4o-mini,gpt-3.5-turbo

# Bedrock Available Models (comma-separated, for validation/listing)
BEDROCK_AVAILABLE_MODELS=us.anthropic.claude-3-sonnet-20240229-v1:0,us.anthropic.claude-3-haiku-20240307-v1:0,us.anthropic.claude-3-opus-20240229-v1:0
BEDROCK_MODELS=us.anthropic.claude-3-sonnet-20240229-v1:0,us.anthropic.claude-3-haiku-20240307-v1:0

# =============================================================================
# OpenAI Configuration
# =============================================================================
# OpenAI API Key (required for OpenAI provider)
OPENAI_API_KEY=sk-your_openai_api_key_here

# OpenAI Model Configuration
OPENAI_MODEL=gpt-4o-mini
OPENAI_PRIMARY_MODEL=gpt-4o-mini

# OpenAI Fallback Models (comma-separated)
OPENAI_FALLBACK_MODELS=gpt-3.5-turbo,gpt-4o

# OpenAI Cross-Provider Fallback (fallback to Bedrock if OpenAI fails)
OPENAI_CROSS_FALLBACK_PROVIDER=bedrock
OPENAI_CROSS_FALLBACK_MODELS=us.anthropic.claude-3-haiku-20240307-v1:0
OPENAI_BEDROCK_FALLBACK_MODELS=us.anthropic.claude-3-haiku-20240307-v1:0

# OpenAI Available Models (comma-separated, for validation/listing)
OPENAI_AVAILABLE_MODELS=gpt-4o-mini,gpt-4o,gpt-3.5-turbo,gpt-4-turbo
OPENAI_MODELS=gpt-4o-mini,gpt-4o,gpt-3.5-turbo

# =============================================================================
# Anthropic Configuration
# =============================================================================
# Anthropic API Key (required for Anthropic provider)
ANTHROPIC_API_KEY=sk-ant-your_anthropic_api_key_here

# Anthropic Model Configuration
ANTHROPIC_PRIMARY_MODEL=claude-3-5-sonnet-20241022

# Anthropic Fallback Models (comma-separated)
# Note: Anthropic fallback models use the same API key

# =============================================================================
# OpenRouter Configuration
# =============================================================================
# OpenRouter API Key (required for OpenRouter provider)
OPEN_ROUTER_API_KEY=sk-or-your_openrouter_api_key_here
OPENROUTER_API_KEY=sk-or-your_openrouter_api_key_here

# OpenRouter Model Configuration
OPENROUTER_PRIMARY_MODEL=x-ai/grok-code-fast-1

# OpenRouter Fallback Models (comma-separated)
OPENROUTER_FALLBACK_MODELS=anthropic/claude-3.5-sonnet,openai/gpt-4o-mini

# OpenRouter Cross-Provider Fallback
OPENROUTER_CROSS_FALLBACK_PROVIDER=openai
OPENROUTER_CROSS_FALLBACK_MODELS=gpt-4o-mini,gpt-3.5-turbo
OPEN_ROUTER_CROSS_FALLBACK_MODELS=gpt-4o-mini,gpt-3.5-turbo

# OpenRouter HTTP Headers (optional, for identification)
OPENROUTER_HTTP_REFERER=https://your-domain.com
OPENROUTER_X_TITLE=Your App Name

# OpenRouter Available Models (comma-separated, for validation/listing)
OPENROUTER_AVAILABLE_MODELS=x-ai/grok-code-fast-1,anthropic/claude-3.5-sonnet,openai/gpt-4o-mini
OPEN_ROUTER_MODELS=x-ai/grok-code-fast-1,anthropic/claude-3.5-sonnet

# =============================================================================
# Google Vertex AI Configuration
# =============================================================================
# Vertex AI API Key (required for Gemini models)
VERTEX_API_KEY=your_vertex_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Vertex AI Project Configuration (required for Anthropic models on Vertex)
VERTEX_PROJECT_ID=your-gcp-project-id
VERTEX_LOCATION_ID=global

# Vertex Model Configuration
VERTEX_PRIMARY_MODEL=gemini-1.5-pro

# Vertex Fallback Models (comma-separated)
VERTEX_FALLBACK_MODELS=gemini-1.5-flash,gemini-pro

# Vertex Anthropic Fallback Models (comma-separated)
VERTEX_ANTHROPIC_FALLBACK_MODELS=claude-3-5-sonnet@20241022,claude-3-opus@20240229

# =============================================================================
# Agent Default Configuration
# =============================================================================
# Default provider to use (bedrock, openai, anthropic, openrouter, vertex)
AGENT_PROVIDER=openrouter

# Default model to use
AGENT_MODEL=x-ai/grok-code-fast-1

# =============================================================================
# Testing Configuration
# =============================================================================
# These variables are used for testing and validation
# Set at least one provider's credentials to run tests

# For Bedrock tests:
# AWS_REGION, AWS_ACCESS_KEY_ID, AWS_SECRET_ACCESS_KEY

# For OpenAI tests:
# OPENAI_API_KEY

# For Anthropic tests:
# ANTHROPIC_API_KEY

# For OpenRouter tests:
# OPEN_ROUTER_API_KEY or OPENROUTER_API_KEY

# For Vertex tests:
# VERTEX_API_KEY or GOOGLE_API_KEY
# VERTEX_PROJECT_ID, VERTEX_LOCATION_ID (for Anthropic models)
